{
  "name": "proxy",
  "version": "0.0.1",
  "description": "Proxy",
  "main": "dist/index.js",
  "repository": "git@github.com:hieudoanm/apis-services.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 14",
    "npm": ">= 8",
    "yarn": "1"
  },
  "scripts": {
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "dev": "kill-port 8081 && concurrently \"nodemon\" \"nodemon -x yarn swagger:json\" \"nodemon -x yarn swagger:yaml\"",
    "docs": "redoc-cli build ./src/swagger.json --output ./public/index.html --title \"Proxy as a Service\"",
    "start": "node .",
    "swagger:json": "tsoa spec-and-routes",
    "swagger:yaml": "json2yaml ./docs/swagger.json > ./docs/swagger.yaml",
    "test": "jest",
    "watch": "webpack --watch"
  },
  "dependencies": {
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "lodash": "4.17.21",
    "pino": "8.3.1",
    "request": "2.88.2",
    "tsoa": "4.1.1"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.12",
    "@types/http-errors": "1.8.2",
    "@types/lodash": "4.14.182",
    "@types/request": "2.48.8",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "@webpack-cli/generators": "2.5.0",
    "eslint": "8.20.0",
    "json2yaml": "1.1.0",
    "redoc-cli": "0.13.16",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-node-externals": "3.0.0"
  }
}
